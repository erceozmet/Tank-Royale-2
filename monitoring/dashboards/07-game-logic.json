  "panels": [
    {
      "id": 1,
      "gridPos": {"h": 8, "w": 12, "x": 0, "y": 0},
      "type": "timeseries",
      "title": "Weapon Pickups Rate",
      "targets": [
        {
          "expr": "rate(game_weapon_pickups_total[1m])",
          "legendFormat": "{{weapon_type}}",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "custom": {
            "drawStyle": "line",
            "lineInterpolation": "smooth",
            "lineWidth": 2,
            "fillOpacity": 10,
            "showPoints": "never",
            "stacking": {"mode": "normal"}
          },
          "unit": "pickups/s",
          "min": 0
        }
      },
      "options": {
        "legend": {
          "displayMode": "list",
          "placement": "bottom"
        },
        "tooltip": {
          "mode": "multi"
        }
      }
