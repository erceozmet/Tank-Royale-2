{
  "id": null,
  "uid": "tank-redis-cache",
  "title": "Tank Royale - Redis Cache",
  "tags": ["tank-royale", "redis", "cache"],
  "timezone": "browser",
  "schemaVersion": 16,
  "version": 1,
  "refresh": "5s",
  "panels": [
    {
      "id": 1,
      "gridPos": {"h": 4, "w": 6, "x": 0, "y": 0},
      "type": "stat",
      "title": "Cache Hit Ratio",
      "targets": [
        {
          "expr": "100 * sum(cache_hits_total) / (sum(cache_hits_total) + sum(cache_misses_total))",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "percent",
          "decimals": 2,
          "thresholds": {
            "mode": "absolute",
            "steps": [
              {"value": 0, "color": "red"},
              {"value": 80, "color": "yellow"},
              {"value": 95, "color": "green"}
            ]
          },
          "mappings": [],
          "min": 0,
          "max": 100
        }
      },
      "options": {
        "orientation": "auto",
        "textMode": "value_and_name",
        "colorMode": "value",
        "graphMode": "area",
        "justifyMode": "auto"
      }
    },
    {
      "id": 2,
      "gridPos": {"h": 4, "w": 6, "x": 6, "y": 0},
      "type": "stat",
      "title": "Total Cache Hits",
      "targets": [
        {
          "expr": "sum(cache_hits_total)",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "short",
          "decimals": 0,
          "thresholds": {
            "mode": "absolute",
            "steps": [
              {"value": 0, "color": "blue"}
            ]
          },
          "color": {"mode": "thresholds"}
        }
      },
      "options": {
        "orientation": "auto",
        "textMode": "value_and_name",
        "colorMode": "value",
        "graphMode": "area"
      }
    },
    {
      "id": 3,
      "gridPos": {"h": 4, "w": 6, "x": 12, "y": 0},
      "type": "stat",
      "title": "Total Cache Misses",
      "targets": [
        {
          "expr": "sum(cache_misses_total)",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "short",
          "decimals": 0,
          "thresholds": {
            "mode": "absolute",
            "steps": [
              {"value": 0, "color": "orange"}
            ]
          },
          "color": {"mode": "thresholds"}
        }
      },
      "options": {
        "orientation": "auto",
        "textMode": "value_and_name",
        "colorMode": "value",
        "graphMode": "area"
      }
    },
    {
      "id": 4,
      "gridPos": {"h": 4, "w": 6, "x": 18, "y": 0},
      "type": "stat",
      "title": "Cache Ops/sec",
      "targets": [
        {
          "expr": "sum(rate(cache_operation_duration_seconds_count[1m]))",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "ops",
          "decimals": 1,
          "thresholds": {
            "mode": "absolute",
            "steps": [
              {"value": 0, "color": "green"},
              {"value": 1000, "color": "yellow"},
              {"value": 5000, "color": "red"}
            ]
          }
        }
      },
      "options": {
        "orientation": "auto",
        "textMode": "value_and_name",
        "colorMode": "value",
        "graphMode": "area"
      }
    },
    {
      "id": 5,
      "gridPos": {"h": 8, "w": 12, "x": 0, "y": 4},
      "type": "timeseries",
      "title": "Cache Hit/Miss Rate Over Time",
      "targets": [
        {
          "expr": "sum(rate(cache_hits_total[1m])) by (cache_type)",
          "legendFormat": "Hits - {{cache_type}}",
          "refId": "A"
        },
        {
          "expr": "sum(rate(cache_misses_total[1m])) by (cache_type)",
          "legendFormat": "Misses - {{cache_type}}",
          "refId": "B"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "custom": {
            "drawStyle": "line",
            "lineInterpolation": "smooth",
            "lineWidth": 2,
            "fillOpacity": 20,
            "showPoints": "never",
            "stacking": {"mode": "none"}
          },
          "unit": "ops",
          "min": 0
        },
        "overrides": [
          {
            "matcher": {"id": "byRegexp", "options": "/Hits.*/"},
            "properties": [
              {"id": "color", "value": {"mode": "fixed", "fixedColor": "green"}}
            ]
          },
          {
            "matcher": {"id": "byRegexp", "options": "/Misses.*/"},
            "properties": [
              {"id": "color", "value": {"mode": "fixed", "fixedColor": "red"}}
            ]
          }
        ]
      },
      "options": {
        "tooltip": {"mode": "multi"},
        "legend": {"displayMode": "list", "placement": "bottom"}
      }
    },
    {
      "id": 6,
      "gridPos": {"h": 8, "w": 12, "x": 12, "y": 4},
      "type": "timeseries",
      "title": "Cache Hit Ratio Trend",
      "targets": [
        {
          "expr": "100 * sum(rate(cache_hits_total[5m])) / (sum(rate(cache_hits_total[5m])) + sum(rate(cache_misses_total[5m])))",
          "legendFormat": "Hit Ratio %",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "custom": {
            "drawStyle": "line",
            "lineInterpolation": "smooth",
            "lineWidth": 3,
            "fillOpacity": 30,
            "showPoints": "auto"
          },
          "unit": "percent",
          "min": 0,
          "max": 100,
          "thresholds": {
            "mode": "absolute",
            "steps": [
              {"value": 0, "color": "transparent"},
              {"value": 80, "color": "red"},
              {"value": 95, "color": "green"}
            ]
          }
        }
      },
      "options": {
        "tooltip": {"mode": "single"},
        "legend": {"displayMode": "list", "placement": "bottom"}
      }
    },
    {
      "id": 7,
      "gridPos": {"h": 8, "w": 12, "x": 0, "y": 12},
      "type": "timeseries",
      "title": "Cache Operation Latency (p50, p95, p99)",
      "targets": [
        {
          "expr": "histogram_quantile(0.50, sum(rate(cache_operation_duration_seconds_bucket[5m])) by (le, operation, cache_type))",
          "legendFormat": "p50 - {{operation}} ({{cache_type}})",
          "refId": "A"
        },
        {
          "expr": "histogram_quantile(0.95, sum(rate(cache_operation_duration_seconds_bucket[5m])) by (le, operation, cache_type))",
          "legendFormat": "p95 - {{operation}} ({{cache_type}})",
          "refId": "B"
        },
        {
          "expr": "histogram_quantile(0.99, sum(rate(cache_operation_duration_seconds_bucket[5m])) by (le, operation, cache_type))",
          "legendFormat": "p99 - {{operation}} ({{cache_type}})",
          "refId": "C"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "custom": {
            "drawStyle": "line",
            "lineInterpolation": "smooth",
            "lineWidth": 2,
            "fillOpacity": 10,
            "showPoints": "never"
          },
          "unit": "s",
          "decimals": 3,
          "min": 0
        },
        "overrides": [
          {
            "matcher": {"id": "byRegexp", "options": "/p50.*/"},
            "properties": [
              {"id": "color", "value": {"mode": "fixed", "fixedColor": "green"}}
            ]
          },
          {
            "matcher": {"id": "byRegexp", "options": "/p95.*/"},
            "properties": [
              {"id": "color", "value": {"mode": "fixed", "fixedColor": "yellow"}}
            ]
          },
          {
            "matcher": {"id": "byRegexp", "options": "/p99.*/"},
            "properties": [
              {"id": "color", "value": {"mode": "fixed", "fixedColor": "red"}}
            ]
          }
        ]
      },
      "options": {
        "tooltip": {"mode": "multi"},
        "legend": {"displayMode": "table", "placement": "right", "calcs": ["lastNotNull", "max"]}
      }
    },
    {
      "id": 8,
      "gridPos": {"h": 8, "w": 12, "x": 12, "y": 12},
      "type": "timeseries",
      "title": "Cache Operations by Type",
      "targets": [
        {
          "expr": "sum(rate(cache_operation_duration_seconds_count[1m])) by (operation, cache_type)",
          "legendFormat": "{{operation}} - {{cache_type}}",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "custom": {
            "drawStyle": "bars",
            "lineInterpolation": "linear",
            "barAlignment": 0,
            "lineWidth": 1,
            "fillOpacity": 80,
            "showPoints": "never",
            "stacking": {"mode": "normal"}
          },
          "unit": "ops",
          "min": 0
        }
      },
      "options": {
        "tooltip": {"mode": "multi"},
        "legend": {"displayMode": "table", "placement": "right", "calcs": ["lastNotNull", "max"]}
      }
    },
    {
      "id": 9,
      "gridPos": {"h": 6, "w": 8, "x": 0, "y": 20},
      "type": "piechart",
      "title": "Cache Hits by Type",
      "targets": [
        {
          "expr": "sum(cache_hits_total) by (cache_type)",
          "legendFormat": "{{cache_type}}",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "short",
          "decimals": 0
        }
      },
      "options": {
        "reduceOptions": {
          "values": false,
          "calcs": ["lastNotNull"]
        },
        "pieType": "pie",
        "tooltip": {"mode": "single"},
        "legend": {"displayMode": "table", "placement": "right", "values": ["value", "percent"]}
      }
    },
    {
      "id": 10,
      "gridPos": {"h": 6, "w": 8, "x": 8, "y": 20},
      "type": "piechart",
      "title": "Cache Misses by Type",
      "targets": [
        {
          "expr": "sum(cache_misses_total) by (cache_type)",
          "legendFormat": "{{cache_type}}",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "short",
          "decimals": 0
        }
      },
      "options": {
        "reduceOptions": {
          "values": false,
          "calcs": ["lastNotNull"]
        },
        "pieType": "pie",
        "tooltip": {"mode": "single"},
        "legend": {"displayMode": "table", "placement": "right", "values": ["value", "percent"]}
      }
    },
    {
      "id": 11,
      "gridPos": {"h": 6, "w": 8, "x": 16, "y": 20},
      "type": "table",
      "title": "Cache Performance Summary",
      "targets": [
        {
          "expr": "sum(cache_hits_total) by (cache_type)",
          "legendFormat": "",
          "refId": "A",
          "format": "table",
          "instant": true
        },
        {
          "expr": "sum(cache_misses_total) by (cache_type)",
          "legendFormat": "",
          "refId": "B",
          "format": "table",
          "instant": true
        },
        {
          "expr": "100 * sum(cache_hits_total) by (cache_type) / (sum(cache_hits_total) by (cache_type) + sum(cache_misses_total) by (cache_type))",
          "legendFormat": "",
          "refId": "C",
          "format": "table",
          "instant": true
        }
      ],
      "transformations": [
        {
          "id": "merge",
          "options": {}
        },
        {
          "id": "organize",
          "options": {
            "renameByName": {
              "cache_type": "Cache Type",
              "Value #A": "Hits",
              "Value #B": "Misses",
              "Value #C": "Hit Ratio %"
            }
          }
        }
      ],
      "fieldConfig": {
        "defaults": {
          "custom": {
            "align": "auto"
          }
        },
        "overrides": [
          {
            "matcher": {"id": "byName", "options": "Hit Ratio %"},
            "properties": [
              {"id": "unit", "value": "percent"},
              {"id": "decimals", "value": 2},
              {"id": "custom.cellOptions", "value": {"type": "color-background"}},
              {"id": "thresholds", "value": {
                "mode": "absolute",
                "steps": [
                  {"value": 0, "color": "red"},
                  {"value": 80, "color": "yellow"},
                  {"value": 95, "color": "green"}
                ]
              }}
            ]
          }
        ]
      }
    },
    {
      "id": 12,
      "gridPos": {"h": 8, "w": 24, "x": 0, "y": 26},
      "type": "timeseries",
      "title": "Cache Operation Latency Heatmap (All Operations)",
      "targets": [
        {
          "expr": "sum(rate(cache_operation_duration_seconds_bucket[5m])) by (le)",
          "legendFormat": "{{le}}",
          "refId": "A",
          "format": "heatmap"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "custom": {
            "drawStyle": "line",
            "lineInterpolation": "smooth",
            "lineWidth": 1,
            "fillOpacity": 50,
            "showPoints": "never"
          },
          "unit": "s"
        }
      },
      "options": {
        "tooltip": {"mode": "multi"},
        "legend": {"displayMode": "hidden"}
      }
    }
  ],
  "time": {
    "from": "now-15m",
    "to": "now"
  },
  "timepicker": {
    "refresh_intervals": ["5s", "10s", "30s", "1m", "5m", "15m"]
  },
  "templating": {
    "list": []
  },
  "annotations": {
    "list": []
  }
}
